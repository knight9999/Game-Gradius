{
  "name": "mt_gradius",
  "version": "1.0.0",
  "description": "Gradius. You have to need packages, uglifyjs, uglifyes, uglifycss",
  "main": "index.js",
  "scripts": {
    "buildjs": "uglifyjs ./src/gradius_canvasimgs.js ./src/gradius_enemies.js ./src/gradius_map.js ./src/gradius_main.js -o ./dist/gradius_main.min.js -c",
    "buildcss": "uglifycss ./src/gradius_main.css > ./dist/gradius_main.min.css",
    "buildhtml": "html-dist --config html-dist.config.js --input ./src/gradius.html",
    "buildjs_edit": "uglifyjs ./src/gradius_canvasimgs.js ./src/gradius_map.js ./src/gradius_editing_stage.js -o ./dist/gradius_editing_stage.min.js -c -m"
  },
  "author": "Takahashi Masaya",
  "license": "ISC",
  "devDependencies": {
    "html-dist": "^0.3.1"
  },
  "dependencies": {
    "uglify-es": "^3.1.6",
    "uglifyjs": "^2.4.11",
    "webpack": "^3.8.1"
  }
}
